<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:visibility="visible"
    android:layout_width="match_parent"
    android:layout_height="150dp">

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imagenICV3Comida"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@color/fondo_oscuro_menosVisibilidad_menos"
        android:fontFamily="@font/gordita"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/stImageGoogleV3"
        app:srcCompat="@drawable/carne"
        app:strokeColor="@color/fondo_oscuro"
        app:strokeWidth="5dp" />


    <TextView
        android:id="@+id/texICV3Nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toEndOf="@+id/imagenICV3Comida"
        app:layout_constraintTop_toTopOf="parent" />

    <RatingBar
        android:id="@+id/ratingICV3Valoracion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toEndOf="@+id/imagenICV3Comida"
        app:layout_constraintTop_toBottomOf="@+id/texICV3Nombre"
        android:isIndicator="true"
        android:numStars="5"
        app:layout_constraintVertical_bias="0.705" />

    <View
        android:id="@+id/divider"
        android:layout_width="4dp"
        android:layout_height="0dp"
        android:backgroundTint="@color/fondo_oscuro"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toStartOf="@+id/ratingICV3Valoracion"
        app:layout_constraintStart_toEndOf="@+id/imagenICV3Comida"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider2"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="@color/fondo_oscuro"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBottom_toTopOf="@id/ratingICV3Valoracion"
        app:layout_constraintStart_toEndOf="@+id/divider"
        app:layout_constraintTop_toBottomOf="@+id/texICV3Nombre" />

    <View
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:backgroundTint="@color/fondo_oscuro"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>